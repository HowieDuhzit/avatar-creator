{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "build/**"
      ]
    },
    "depcheck": {
      "cache": true
    },
    "iterate": {
      "cache": false
    },
    "lint": {
      "cache": true
    },
    "lint-fix": {
      "cache": true
    },
    "pre:build": {
      "dependsOn": [
        "^pre:build"
      ],
      "outputs": [
        "build/**"
      ],
      "cache": true
    },
    "type-check": {
      "cache": true
    }
  }
}
