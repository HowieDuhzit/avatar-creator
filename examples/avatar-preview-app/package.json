{
  "name": "@private/avatar-preview-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@msquared/avatar-creator": "*",
    "highlight.js": "^11.11.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "dev": "node build.mjs --watch & npx serve@latest -l 3000 ./build",
    "build": "node build.mjs --build",
    "start": "npx serve@latest ./build",
    "lint": "eslint \"./**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "lint-fix": "eslint \"./**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "9.27.0",
    "@types/node": "22.10.2",
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.4",
    "esbuild": "0.24.0",
    "esbuild-plugin-copy": "2.1.1",
    "eslint": "9.27.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.1"
  }
}
